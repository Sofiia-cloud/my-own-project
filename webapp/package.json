{
  "name": "@my-project/webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "types": "tsc --noEmit --project ./tsconfig.app.json && tsc --noEmit --project ./tsconfig.node.json",
    "prettify": "prettier --log-level warn --cache --write '{src/**/*,*}.{ts,tsx,js,json,yml,scss}' '!**/{node_modules,dist}/**/*'",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts,.tsx .",
    "stylelint": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache src/**/*.scss",
    "stylecheck": "sass --update -q ./src:./node_modules/.cache/.stylecheckcache"
  },
  "dependencies": {
    "@my-project/backend": "workspace:*",
    "@tanstack/react-query": "^5.90.5",
    "@trpc/client": "^11.6.0",
    "@trpc/react-query": "^11.6.0",
    "formik": "^2.4.9",
    "formik-validator-zod": "^2.2.0",
    "include-media": "^2.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5",
    "reset-css": "^5.0.2",
    "saas": "^1.0.0",
    "zod": "^4.1.12",
    "zod-formik-adapter": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "globals": "^16.4.0",
    "prettier": "3.0.1",
    "sass-embedded": "^1.93.3",
    "stylelint": "^16.25.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "typescript": "~5.9.3",
    "vite": "^7.1.7"
  }
}
