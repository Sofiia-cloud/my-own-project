  "dependencies": {
    "@my-project/backend": "workspace:*",
    "@tanstack/react-query": "^5.90.5",
    "@trpc/client": "^11.6.0",
    "@trpc/react-query": "^11.6.0",
    "react": "^19.1.1",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5"
  },
  "devDependencies": {
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "globals": "^16.4.0",
    "prettier": "3.0.1",
    "typescript": "~5.9.3",
    "vite": "^7.1.7"
